                },
                "message": {
                    "type": "string"
                }
            }
        },
        "api.ListProjectsRsp": {
            "type": "object",
            "properties": {
                "nextPageToken": {
                    "description": "`next_page_token` can be sent in a subsequent call to fetch more results",
                    "type": "string",
                    "example": "498c7f44-656f-4944-b455-0695faa5d4f9"
                },
                "projects": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/api.Project"
                    }
                }
            }
        },
        "api.ListSqlUsersRsp": {
            "type": "object",
            "properties": {
                "nextPageToken": {
                    "description": "`next_page_token` can be sent in a subsequent call to fetch more results",
                    "type": "string",
                    "example": "498c7f44-656f-4944-b455-0695faa5d4f9"
                },
                "sqlUsers": {
                    "description": "SqlUsers []*SqlUser `json:\"sqlUsers\"`",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/api.SqlUser"
                    }
                }
            }
        },
        "api.OpenApiCreateMspCustomerSignupUrlReq": {
            "type": "object",
            "properties": {
                "orgId": {
                    "description": "The ID of the MSP.",
                    "type": "string",
                    "example": "123456"
                }
            }
        },
        "api.OpenApiError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "error": {},
                "msgPrefix": {
                    "type": "string"
                },
                "status": {
                    "type": "integer"
                }
            }
        },
        "api.OpenApiListMspCustomerRsp": {
            "type": "object",
            "properties": {
                "mspCustomers": {
                    "description": "The list of matching MSP Customers.",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/api.OpenApiMspCustomer"
                    }
                },
                "nextPageToken": {
                    "description": "`next_page_token` can be sent in a subsequent call to fetch more results",
                    "type": "string",
                    "example": "498c7f44-656f-4944-b455-0695faa5d4f9"
                }
            }
        },
        "api.OpenApiMspCustomer": {
            "type": "object",
            "properties": {
                "effectedTime": {
                    "description": "The effected time of the MSP customer.",
                    "type": "string",
                    "example": "2023-12-06 15:18:13"
                },
                "email": {
                    "description": "The email of the MSP customer.",
                    "type": "string",
                    "example": "123@my.com"
                },
                "expiredTime": {
                    "description": "The expired time of the MSP customer.",
                    "type": "string",
                    "example": "2023-12-06 15:18:13"
                },
                "id": {
                    "description": "The ID of the MSP customer.",
                    "type": "string",
                    "example": "123456"
                },
                "orgId": {
                    "description": "The ID of the MSP customer.",
                    "type": "string",
                    "example": "123456"
                },
                "orgName": {
                    "description": "The org name of the MSP customer.",
                    "type": "string",
                    "example": "New Org"
                },
                "state": {
                    "description": "The state of the MSP customer.",
                    "type": "string",
                    "example": "active"
                }
            }
        },
        "api.OpenApiMspCustomerSignupUrl": {
            "type": "object",
            "properties": {
                "customerSignupUrl": {
                    "description": "The signup URL of the MSP customer.",
                    "type": "string",
                    "example": "https://test.com/signup?msp=test\u0026third_party_account_provider=test\u0026third_party_account_token=test"
                },
                "expiredAt": {
                    "description": "The expired time of the MSP customer signup url.",
                    "type": "string",
                    "example": "2024-12-06 15:18:13"
                }
            }
        },
        "api.Project": {
            "type": "object",
            "properties": {
                "aws_cmek_enabled": {
                    "description": "The AwsCmekEnabled of the project.",
                    "type": "boolean",
                    "example": false
                },
                "cluster_count": {
                    "description": "Number of cluster_ in the project.",
                    "type": "integer",
                    "example": 7
                },
                "create_timestamp": {
                    "description": "The create time key of the project.",
                    "type": "string",
                    "example": "1688460316"
                },
                "id": {
                    "description": "The id of the project.",
                    "type": "string",
                    "example": "1234566"
                },
                "name": {
                    "description": "The name of the API key.",
                    "type": "string",
                    "example": "project01"
                },
                "org_id": {
                    "description": "The org id  of the project.",
                    "type": "string",
                    "example": "324234234"
                },
                "user_count": {
                    "description": "Number of users in the project.",
                    "type": "integer",
                    "example": 4
                }
            }
        },
        "api.SqlUser": {
            "type": "object",
            "properties": {
                "authMethod": {
                    "type": "string"
                },
                "builtinRole": {
                    "type": "string"
                },
                "customRoles": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "userName": {
                    "type": "string"
                }
            }
        },
        "central.SqlUser": {
            "type": "object",
            "properties": {
                "auth_method": {
                    "type": "string"
                },
                "builtin_role": {
                    "type": "string"
                },
                "custom_role": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "user_name": {
                    "type": "string"
                }
            }
