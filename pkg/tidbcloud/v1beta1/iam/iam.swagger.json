                "summary": "update one sql user",
                "parameters": [
                    {
                        "type": "string",
                        "description": "The name of the sql user.",
                        "name": "userName",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/central.SqlUser"
