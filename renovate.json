{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies"],      
  "vulnerabilityAlerts": {  
    "enabled": true
  },
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"]
    }
  ], 
  "baseBranches": ["main", "/^release\\/.*/"] 
}
